@model IEnumerable<SHABAEIKO_ORGANIC_FARM.Models.User>

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Customers</h2>
        </div>
        <div class="col-6">
            <a asp-controller="Item" asp-action="Details" class="btn btn-info">Back</a>
        </div>
    </div>
</div>
<br />
<p> Registered Succesfully </p>

@if (Model.Count() > 0)
{
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Username
                </th>


            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td width="30%">@user.User_Id</td>
                    <td width="30%">@user.Username</td>




                </tr>

            }
        </tbody>
    </table>
}
else
{
    <p> No Customers List </p>
}
