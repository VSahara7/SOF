@model SHABAEIKO_ORGANIC_FARM.Models.User

<form method="post" asp-action="Create" enctype="multipart/form-data">
    <div class="border p-4">
        <div class="form-group row">
            <h2 class="text-black-50" pl-3>Register</h2>
        </div>
        <div class="row">
            <div class="col-30">
                <div class="form-group row">


                    <div class="col-2">
                        <label asp-for="Username"></label>
                    </div>
                    <br />
                    <div class="col-2">
                        <label asp-for="Address"></label>
                    </div>
                    <br />
                    <div class="col-2">
                        <label asp-for="Contact"></label>
                    </div>
                    <br />
                    <div class="col-2">
                        <label asp-for="Email"></label>
                    </div>
                    <br />
                    <div class="col-2">
                        <label asp-for="Password"></label>
                    </div>
                </div>

                <div class="form-group row">
                    <div class="col-2">
                        <input asp-for="Username" class="form-control" />
                    </div>
                    <br />
                    <div class="col-2">
                        <input asp-for="Address" class="form-control" />
                    </div>
                    <br />
                    <div class="col-2">
                        <input asp-for="Contact" class="form-control" />
                    </div>
                    <br />
                    <div class="col-2">
                        <input asp-for="Email" class="form-control" />
                    </div>
                    <br />
                    <div class="col-2">
                        <input asp-for="Password" class="form-control" />
                    </div>

                </div>
                <div class="form-group row">
                    <div class="col-8 offset-2 row">
                        <div class="col">
                            <input type="submit" class="btn btn-info w-75" value="Register" />
                        </div>
                        <div class="col">
                            <a asp-action="create" class="btn btn-danger w-75">Cancel</a>
                        </div>
                      

                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
